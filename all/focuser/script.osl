permission "request" "terminal"
permission "request" "javascript"
permission "request" "start run"
window "hide"
window "responsive" true
window.width = 0
window.height = 0

mainloop:
  name = Windows()[1].name
  if temp[1] == name (
    exit
  )
  temp = name
  if name != "focuser" (
    eval "document.title = 'originOS > " ++ name ++ "'"
  ) else (
    eval "document.title = 'originOS'"
  )
